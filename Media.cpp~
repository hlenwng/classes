#include <iostream>
#include <string>
#include "Media.h"
using namespace std;

Media::Media() {
  title = new char[60];
}

Media::Media(char*t, int y) {
  title = new char[60];
  strcpy(title, t);
  year = y;
}

char* Media::getTitle() {
  return title;
}

int Media::getYear() {
  return year;
}

void Media::printM() {
  cout << "Title: " << title << endl;
  cout << "Year: " << year << endl;
}
