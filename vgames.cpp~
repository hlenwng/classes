#include <iostream>
#include "vgames.h"
using namespace std;

vgames::vgames(char*t, int y, char*p, int r):Media(t,y) {
  //cout << "vgames contructor" << endl;
  publisher = new char[20];
  strcpy(publisher, p);

  rating = r;
}


vgames::~vgames() {
  delete[] publisher;
}


char* vgames::getPublisher() {
  return publisher;
}

int vgames::getRating() {
  return rating;
}

void vgames::printV() {
  cout << "Publisher: " << publisher << endl;
  cout << "Rating: " << rating << endl;
}
