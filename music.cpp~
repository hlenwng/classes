#include <iostream>
#include "Music.h"
using namespace std;

music::music(char*t, char*a, int y, int d, char*p):Media(t,y) {
  //cout << "music contructor" << endl;
  artist = new char[20];
  strcpy(artist, a);

  duration = d;

  publisher = new char[20];
  strcpy(publisher, p);
}

music::~music() {
  delete[] artist;
  delete[] publisher;
}

char* music::getArtist() {
  return artist;
}

char* music::getPublisher() {
  return publisher;
}

int music::getDuration() {
  return duration;
}

